{{elem/panel-message
  successMessage=this.successMessage
  warningMessage=this.warningMessage
  errorMessage=this.errorMessage}}

{{#if this.isProcessing}}
  {{#elem/panel-form
    name="info"
    heading="Processing..." }}
      {{this.progressMsg}}
  {{/elem/panel-form}}
{{/if}}

  <h5>Upload Datasets (JSON / gzip, xlsx, ods)</h5>
  <form {{action 'submitFile' on='submit'}}>
    <input
      type='file'
      accept=".json,.gz,.xlsx,.xls,.ods"
      onchange={{action 'setFile'}}>
    <br>
    <input
      type="submit"
      class="btn btn-primary {{if this.isProcessing 'disabled'}}">
  </form>
