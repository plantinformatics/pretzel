{{!-- log 'entry-parent' name values this this.parentView --}}
{{#record/entry-selectable
  node=this.node
  nodeName=this.name
  selectedNode=this.selectedDataset
  selectNode=this.selectDataset
}}

  {{#record/entry-expander nodeName=this.name}}
    <ul>
      {{record/entry-values
        values=this.values
        levelMeta=this.levelMeta
        name=this.name
        parentDone=true
        controlOptions=this.controlOptions
        loadBlock=this.loadBlock
        selectBlock=this.selectBlock
        selectedBlock=this.selectedBlock
        selectDataset=this.selectDataset
      }}
    </ul>

  {{/record/entry-expander}}

{{/record/entry-selectable}}

