{{!-- values is an array of blocks --}}

{{!-- log 'entry-scope' name values this this.parentView --}}
{{#record/entry-selectable
  node=this.node
  nodeName=this.name
  parentName=this.parentName
  selectedNode=this.selectedDataset
  selectNode=this.selectDataset
}}

  {{#record/entry-expander nodeName=this.name hoverText=(or this.parentName this.name)}}

    {{record/entry-blocks
      loadBlock=this.loadBlock
      data=this.values
      selectBlock=this.selectBlock
      selectedBlock=this.selectedBlock
      selectDataset=this.selectDataset}}

  {{/record/entry-expander}}

{{/record/entry-selectable}}
