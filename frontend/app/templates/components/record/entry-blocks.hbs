<ul>
  {{#each this.data as |block|}}
    {{!-- log  'entry-blocks' 'block.name' block.name --}}
    <li class="list-group-item  {{if (compare block.id '===' this.selectedBlock.id) 'list-group-item-warning'}}" {{action "selectBlockAndDataset" block on="click"}} style="display: inline-block;">
      {{record/entry-block-by-scope
        loadBlock=this.loadBlock
        selectDataset=this.selectDataset
        entry=block }}
      {{!--
      {{block.datasetId.name}}
      <span class="blockInScope">{{block.name}}</span>
      --}}
    </li>
  {{/each}}
</ul>
