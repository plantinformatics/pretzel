{{record/entry-record
  entry=this.entry}}

<h5> {{elem/icon-base name="arrow-up"}}
  <span class="label label-info">
    {{!-- alternative : use {{object-at 0 this.entry.features}} --}}
    {{elem/icon-base name="asterisk"}} {{this.entry.features.[0]}} {{this.entry.positions.[0]}}
  </span>
</h5>
<h5> {{elem/icon-base name="arrow-down"}}
  <span class="label label-info">
    {{elem/icon-base name="asterisk"}} {{this.entry.features.[1]}} {{this.entry.positions.[1]}}
  </span>
</h5>
