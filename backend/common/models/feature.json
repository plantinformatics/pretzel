{
  "name": "Feature",
  "base": "PersistedModel",
  "description": "Unit of data that can contain other units",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "range": {
      "type": ["number"],
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "block": {
      "type": "belongsTo",
      "model": "Block"
    },
    "parent": {
      "type": "belongsTo",
      "model": "Feature"
    },
    "features": {
      "type": "hasMany",
      "model": "Feature",
      "foreignKey": "parentId"
    }
  },
  "acls": [
  ],
  "methods": {}
}
